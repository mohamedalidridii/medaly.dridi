{
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|map))",
      "dest": "/$1"
    },
    {
      "src": "/site-icon.png",
      "dest": "/site-icon.png"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
}
